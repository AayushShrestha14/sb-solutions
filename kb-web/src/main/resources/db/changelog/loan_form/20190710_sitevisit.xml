<databaseChangeLog
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

  <changeSet id="23-create-site-visit" author="elvin">
    <createTable tableName="site_visit">
      <column name="id" type="BIGINT" autoIncrement="true">
        <constraints nullable="false" primaryKey="true"/>
      </column>
      <column name="hasCurrentResident" type="BOOLEAN"/>
      <column name="hasBusinessSiteVisit" type="BOOLEAN"/>
      <column name="hasFixedAssetsCollateral" type="BOOLEAN"/>
      <column name="hasCurrentAssetsInspection" type="BOOLEAN"/>
    </createTable>
  </changeSet>

</databaseChangeLog>