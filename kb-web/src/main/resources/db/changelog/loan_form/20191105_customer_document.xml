<databaseChangeLog
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
  <changeSet id="47-create-table-customer-document" author="yunish">
    <createTable tableName="customer_document">
      <column name="id" type="BIGINT" autoIncrement="true">
        <constraints nullable="false" primaryKey="true"/>
      </column>
      <column name="document_path" type="VARCHAR(256)"/>
      <column name="documents_id" type="BIGINT">
        <constraints nullable="false"/>
      </column>
    </createTable>
    <addForeignKeyConstraint baseTableName="customer_document"
      baseColumnNames="documents_id"
      constraintName="fk_customer_document_document_id"
      referencedTableName="document"
      referencedColumnNames="id"/>

  </changeSet>
</databaseChangeLog>
